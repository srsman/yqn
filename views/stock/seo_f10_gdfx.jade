//- 股东分析
extends seo_f10_layout

block seo_f10_menu
  +f10Menu('gdfx')

block f10_content
  //- 日期列表
  if gdfxInfo.dateList && gdfxInfo.dateList.length > 0
    ul.seo-stock-f10-second-menu
      - for(var i = 0; i < 5; i++) {
          li
            a(href='/gupiao/#{stockInfo[0]}/GDFX/#{gdfxInfo.dateList[i]}') #{gdfxInfo.dateList[i]}
      - }

    if gdfxInfo.top10FloatHolder || gdfxInfo.top10Holder || gdfxInfo.holderAlter
      //- 十大流动股东
      if gdfxInfo.top10FloatHolder
        h1.qn-table-title 十大流通股东
        table.qn-table.striped
          thead
            tr
              - for(var i = 0; i < gdfxInfo.top10FloatHolder[0].length; i++) {
                  th #{gdfxInfo.top10FloatHolder[0][i]}
              - }
          tbody
            - for(var j = 1; j < gdfxInfo.top10FloatHolder.length; j++) {
                tr
                  - for(var i = 0; i < gdfxInfo.top10FloatHolder[j].length; i++) {
                      td #{gdfxInfo.top10FloatHolder[j][i]}
                  - }
            - }

      //- 十大股东
      if gdfxInfo.top10Holder
        h1.qn-table-title 十大股东
        table.qn-table.striped
          thead
            tr
              - for(var i = 0; i < gdfxInfo.top10Holder[0].length; i++) {
                  th #{gdfxInfo.top10Holder[0][i]}
              - }
          tbody
            - for(var j = 1; j < gdfxInfo.top10Holder.length; j++) {
                tr
                  - for(var i = 0; i < gdfxInfo.top10Holder[j].length; i++) {
                      td #{gdfxInfo.top10Holder[j][i]}
                  - }
            - }

      //- 董坚高持股变化
      if gdfxInfo.holderAlter
        h1.qn-table-title 董坚高持股变化
        table.qn-table.striped
          thead
            tr
              - for(var i = 0; i < gdfxInfo.holderAlter[0].length; i++) {
                  th #{gdfxInfo.holderAlter[0][i]}
              - }
          tbody
            - for(var j = 1; j < gdfxInfo.holderAlter.length; j++) {
                tr
                  - for(var i = 0; i < gdfxInfo.holderAlter[j].length; i++) {
                      td #{gdfxInfo.holderAlter[j][i]}
                  - }
            - }
    else
      p 暂无数据
